value = 0

[state 0, 2]
type = velset
trigger1 = time = 0
x = 0
y = 0

[state 0, 3]	; 死了么？
type = changestate
trigger1 = !alive
value = 5050

[state 0, 4]
type = destroyself
trigger1 = ishelper

[state 0, 5]
type = afterimage
trigger1 = time = 0
time = 1

[state 0, 6]
type = turn
trigger1 = var(59) && !ctrl
trigger1 = teammode = single || teammode = turns
trigger1 = p2dist X < 0

