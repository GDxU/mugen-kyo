[state 40, 1]		; 防止AI乱动
type = changestate
trigger1 = var(59) && time = 0
value = 0
ctrl = 1

[state 40, 2]
type = NotHitBy
trigger1 = 1
value = , AT		; SCA, AT简写

[state 40, 3]
type = afterimage
trigger1 = time = 0
time = 1

[state 40, 4]
type = varset
trigger1 = time = 0
sysvar(1) = 0		; 清零临时变量

[state 40, 5]
type = varset
trigger1 = time = 0
trigger1 = command = "holdfwd"
